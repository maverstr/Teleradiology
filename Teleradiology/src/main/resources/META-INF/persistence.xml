<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Teleradiology" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Controller.Practitionerdicomidentifier</class>
    <class>Controller.Patientdicomidentifier</class>
    <class>Controller.Medicalstaff</class>
    <class>Controller.User</class>
    <class>Controller.Instance</class>
    <class>Controller.Person</class>
    <class>Controller.Series</class>
    <class>Controller.Imagingstudy</class>
    <class>Controller.Report</class>
    <class>Controller.Patient</class>
    <class>Controller.Radiologist</class>
    <class>model.Practitionerdicomidentifier</class>
    <class>model.Patient</class>
    <class>model.Report</class>
    <class>model.User</class>
    <class>model.Imagingstudy</class>
    <class>model.Series</class>
    <class>model.Radiologist</class>
    <class>model.Patientdicomidentifier</class>
    <class>model.Medicalstaff</class>
    <class>model.Instance</class>
    <class>model.Person</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/teleradiology?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
